{
    "reporters": [
        "default",
        [
            "./node_modules/jest-html-reporter",
            {
                "pageTitle": "Unit Test Report",
                "outputPath": "test-reports/test-report.html"
            }
        ]
    ],
    "collectCoverageFrom": [
        "src/**/*.{js,jsx}",
        "!<rootDir>/node_modules/",
        "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
        "global": {
            "branches": 90,
            "functions": 90,
            "lines": 90,
            "statements": 90
        }
    },
    "coverageReporters": [
        "html"
    ]
}